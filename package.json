{
  "name": "google-books",
  "version": "1.0.0",
  "description": "Search for books with the MERN Stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodmeon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LTo28/google-books.git"
  },
  "author": "Larry To",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LTo28/google-books/issues"
  },
  "homepage": "https://github.com/LTo28/google-books#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.5"
  }
}
